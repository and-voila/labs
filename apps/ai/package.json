{
  "name": "@av/ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "prisma generate && pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "prisma generate && pnpm with-env next dev --port 3001",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "generate": "pnpm with-env prisma generate",
    "knip": "knip",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "start": "pnpm with-env prisma generate && pnpm with-env next start",
    "stripe": "stripe listen --forward-to localhost:3001/api/webhook",
    "studio": "pnpm with-env prisma studio",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "with-env-production": "dotenv -e ../../.env.production --"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@av/db": "workspace:^0.1.0",
